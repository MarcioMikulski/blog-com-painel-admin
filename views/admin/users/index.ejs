<%- include ('../../partials/header.ejs') %>
<%- include ('../../partials/navbar.ejs') %>

<div class="container">
<hr>
<h2>Usuários</h2>
<a class="btn btn-secondary" href="/admin/users/create">Criar novo usuário</a>
<hr>
<table class="table table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user =>{ %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
        </tr>
<% }) %>
    </tbody>
</table>







</div>











